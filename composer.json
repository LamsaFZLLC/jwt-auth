{
    "name": "lamsa/jwt-decoder",
    "description": "JWT encoder to be used for microservices",
    "authors": [
        {
            "name": "Abdelhameed al-Asbahi",
            "email": "abdkwa92@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lamsa\\JwtDecoder\\": "src/"
        }

    },
    "require": {
        "php": ">=7.1",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nixilla/php-jwt": "dev-master",
        "symfony/flex": "^1.16"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1TW989CK77ZA7B2H4HC9WAG",
            "allow-contrib": true,
            "require": "5.3.*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
